@page "/listadotitulares"
@inject ListarTitularesUseCase ListarTitularesUseCase

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>APELLIDO</th>
            <th>NOMBRE</th>
            <th>DNI</th>
            <th>TELEFONO</th>
            <th>DIRECCION</th>
            <th>EMAIL</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var t in _lista)
        {
            <tr>
                <td>@t.Id</td>
                <td>@t.Apellido</td>
                <td>@t.Nombre</td>
                <td>@t.Dni</td>
                <td>@t.Telefono</td>
                <td>@t.Direccion</td>
                <td>@t.Email</td>
            </tr>
        }
    </tbody>
</table>

@code
{
    List<Titular> _lista = new List<Titular>();
    protected override void OnInitialized()
    {
        _lista = ListarTitularesUseCase.Ejecutar();
    }
}